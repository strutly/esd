<view class="score">3</view>
<view class="sum">总分</view>

<view class="tag">症状</view>

<view class="summary">
  <view class="summary_lable">严重便秘</view>
  <view class="summary_val">2</view>
</view>
<block wx:for="{{questions}}" wx:for-index="key" wx:key="index" wx:for-item="question">
  <view class="tag">{{key}}</view>
  <view class="form-data">
    <view class="form-item" wx:for="{{question}}" wx:key="k" wx:for-index="k" wx:for-item="check">
      <view class="item-question">
        <view class="item-question-title">{{k}}</view>
        <view class="item-question-score score-{{check.score}}">{{check.score}}</view>
      </view>
      <view class="item-question-answer answer-{{check.score}}">{{check.lable}}</view>
    </view>
  </view>
</block>
